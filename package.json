    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.4",
    "jest": "^18.1.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "react-test-renderer": "^15.4.2",
