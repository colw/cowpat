  },
  "homepage": "https://github.com/colw/nxws",
  "dependencies": {
    "express": "^4.13.3",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "next"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.1.20",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.22.0",
