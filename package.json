{
  "name": "nxws",
  "version": "0.1.0",
  "description": "A simple news aggregator.",
  "main": "app.js",
  "scripts": {
    "start": "webpack && node app.js",
    "dev": "webpack-dev-server --hot --inline --port 8081",
